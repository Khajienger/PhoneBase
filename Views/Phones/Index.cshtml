@model List<PhoneRecord>
@{
    <table>
        <thead>
            <tr>
    
                <th>ID</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>

            </tr>
        </thead>

        <tbody>

            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td><a href="phones/phonerecorddetails">@i.ToString()</a></td>
                    <td>@Model[i].surname</td>
                    <td>@Model[i].name</td>
                    <td>@Model[i].patronymic</td>

                </tr>
            }

        </tbody>

    </table>
}