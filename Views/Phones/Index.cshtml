@model List<PhoneRecord>
@{


    <table align="center">
        <thead>
            <tr>

                <th style="border:solid">ID</th>
                <th style="border:solid">Фамилия</th>
                <th style="border:solid">Имя</th>
                <th style="border:solid"> Отчество</th>

            </tr>
        </thead>




        <tbody>

            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td><a href="@Url.Action("phonerecorddetails", "Phones", new {id = Model[i].id})">@Model[i].id.ToString()</a></td>
                    <td>@Model[i].surname</td>
                    <td>@Model[i].name</td>
                    <td>@Model[i].patronymic</td>

                </tr>
            }

        </tbody>

    </table>
}